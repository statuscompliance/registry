{
  "name": "governify-registry",
  "buildOn": "2018-04-18",
  "version": "3.8.0",
  "description": "Central repository for agreements",
  "homepage": "https://github.com/isa-group/governify-registry",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.9.0"
  },
  "main": "index.js",
  "keywords": [
    "governify",
    "registry",
    "agreements"
  ],
  "license": "GPL-2.0",
  "private": true,
  "dependencies": {
    "@oas-tools/oas-telemetry": "0.5.1",
    "ajv": "8.17.1",
    "bluebird": "3.7.2",
    "body-parser": "1.20.3",
    "compression": "1.7.5",
    "cors": "2.8.5",
    "deep-diff": "1.0.2",
    "elastic-apm-node": "4.8.1",
    "express": "4.21.2",
    "governify-agreement-manager": "0.0.9",
    "governify-commons": "1.19.0",
    "helmet": "8.0.0",
    "js-yaml": "4.1.0",
    "json-schema-ref-parser": "9.0.9",
    "JSONStream": "1.3.5",
    "moment-timezone": "0.5.46",
    "mongoose": "8.8.4",
    "nodemailer": "6.9.16",
    "nodemailer-sendgrid-transport": "0.2.0",
    "oas-tools": "3.1.0",
    "rrule": "2.8.1",
    "swagger-tools": "0.10.4",
    "swagger-ui-express": "5.0.1",
    "v8": "0.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "@stylistic/eslint-plugin-js": "2.12.1",
    "eslint": "9.17.0",
    "globals": "15.13.0",
    "mocha": "11.0.1",
    "mongodb-memory-server": "10.1.2",
    "nodemon": "3.1.9",
    "nyc": "17.1.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch --env-file=.env index.js",
    "ci": "node --watch --env-file=.env.example index.js",
    "test": "mocha ./tests/test.js --timeout 20000 -s 20",
    "coverage": "nyc --reporter=lcov mocha ./tests/test.js --timeout 20000 -s 20",
    "lint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint --ignore-pattern tests",
    "lint-fix": "eslint . --fix --max-warnings=0 --cache --cache-location node_modules/.cache/eslint --ignore-pattern tests",
    "old_dev": "concurrently --kill-others \"yarn run start\" \"grunt watch\"",
    "old_watch": "grunt watch",
    "old_test": "grunt test",
    "old_coverage": "grunt coverage",
    "old_build": "grunt build",
    "old_release:patch": "grunt release",
    "old_release:minor": "grunt release:minor",
    "old_release:major": "grunt release:major",
    "old_deliver": "grunt deliver"
  },
  "author": {
    "name": "ISA group",
    "web": "http://www.isa.us.es/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/governify/registry"
  },
  "docker": {
    "url": "https://hub.docker.com/r/governify/registry"
  }
}
